{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "OpenAI": {
    "ApiKey": "${OPENAI_API_KEY}",
    "ModelName": "${OPENAI_MODEL:gpt-3.5-turbo}",
    "EmbeddingModelName": "${OPENAI_EMBEDDING_MODEL:text-embedding-3-small}"
  },
  "FluxIndex": {
    "DatabasePath": "./data/fluxindex_demo.db",
    "UseSQLiteVec": true,
    "VectorDimension": 1536,
    "FallbackToInMemoryOnError": false
  },
  "Demo": {
    "TestDataPath": "D:/test-filer",
    "MaxDocuments": 100,
    "SearchTopK": 10
  }
}