{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FluxIndex": {
    "Database": {
      "Path": "integration_test.db",
      "AutoMigrate": true
    },
    "OpenAI": {
      "EmbeddingDimensions": 1536,
      "MaxRetries": 3,
      "Timeout": 30
    },
    "FileFlux": {
      "ChunkingStrategy": "Auto",
      "MaxChunkSize": 512,
      "OverlapSize": 64
    },
    "WebFlux": {
      "MaxDepth": 2,
      "FollowExternalLinks": false,
      "Timeout": 30
    }
  }
}